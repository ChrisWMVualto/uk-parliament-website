@model UKP.Website.Models.Event.EventViewModel


<script src="~/Scripts/jquery.signalR-2.1.1.min.js"></script>
<script src="~/signalr/hubs"></script>
<script src="~/Content/js/event.js"></script>

<div class="container main-container">

    @Html.Hidden("eventId", Model.VideoModel.EventModel.Id)

    <div id="eventTimesContainer" data-load-url="@Url.Action(MVC.Event.EventTitle(Model.VideoModel.EventModel.Id))"></div>
    <div class="row">
        <div class="col-md-12">
            <header class="title-main"><h2><i class="fa fa-play-circle fa-lg"></i> <span>Player</span></h2>     <a href="#" class="rss-link pull-right hidden-xs">RSS <i class="fa fa-rss fa-2x"></i></a></header>
        </div>

        <div class="col-md-12 page-title-bar">
            <div class="row">
                @Html.Action(MVC.Event.EventTitle())
                <div class="col-md-2">
                    <span class="state-badge pending-badge visible-lg visible-md">Vod</span>
                </div>
            </div>
        </div>


        <div class="col-md-12">
            <div class="row">


                <div class="col-md-7">

                    <div class="video-container">
                        <img alt="" class="img-responsive" src="img/filler-player-image.jpg" />

