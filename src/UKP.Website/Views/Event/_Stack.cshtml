@using UKP.Website.Service.Model
@model VideoModel
<div class="stack-player">
    <div class="stack">
        <ol>
            @foreach(var stack in Model.Stacks)
            {
                <li>
                    <article>
                        <span class="vr-stack"></span>
                        <span class="stack-number">3</span>
                        <header class="stack-item">
                            <h4>
                                @stack.Title
                            </h4>
                        </header>
                    </article>
                </li>
            }
        </ol>
    </div>
</div>
